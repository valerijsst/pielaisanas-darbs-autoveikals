<h1>Autoveikals</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Title</th>
      <th>Price</th>
      <th>Quantity</th>   
    </tr>
  </thead>

  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= image_tag product.image.url, style: "width: 100px;" if product.image.present? %></td>
        <td><%= product.title %></td>
        <td><%= product.price %> $</td>
        <td><%= product.quantity %></td>
      </tr>
    <% end %>
  </tbody>
</table>